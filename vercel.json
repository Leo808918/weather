{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/chat",
      "dest": "/api/chat.py"
    },
    {
      "src": "/api/check",
      "dest": "/api/check.py"
    },
    {
      "src": "/api/entries",
      "dest": "/api/entries.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}

